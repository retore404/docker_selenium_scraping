{
    // コンテナの名前
    "name": "dev container",

    // Docker Composeの設定ファイルパス
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],

    // コンテナのサービス名
    "service": "app",

    // VSCodeで開くコンテナ内のパス
    "workspaceFolder": "/app",

    // コンテナ内で使用する拡張機能
    "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
    ]
}